{
    "format": ["Milhoja-JSON", "1.0.0"],
    "name":                "hydro_op1",
    "variable_index_base":           1,
    "external": {
        "_dt":      {"type":        "real",
                     "extents":     "()"}
    },
    "scratch": {
        "_auxC":    {"type":        "real",
                     "extents":     "REPLACE ME!",
                     "lbound":      "REPLACE ME!"}
    },
    "hy::computeFluxesHll": {
        "interface_file": "Hydro.h",
        "argument_list": ["dt", "lo", "hi", "deltas", "Uin", "flX", "flY", "flZ", "auxC"],
        "argument_specifications": {
            "dt":        {"source":             "external",
                          "name":               "_dt"},
            "lo":        {"source":             "tile_lo"},
            "hi":        {"source":             "tile_hi"},
            "deltas":    {"source":             "tile_deltas"},
            "Uin":       {"source":             "grid_data",
                          "structure_index":    ["CENTER", 1],
                          "R":                  [1, 2, 3, 4, 5, 6, 9]},
            "flX":       {"source":             "grid_data",
                          "structure_index":    ["FLUXX", 1],
                          "W":                  [1, 2, 3, 4, 5]},
            "flY":       {"source":             "grid_data",
                          "structure_index":    ["FLUXY", 1],
                          "W":                  [1, 2, 3, 4, 5]},
            "flZ":       {"source":             "grid_data",
                          "structure_index":    ["FLUXZ", 1],
                          "W":                  [1, 2, 3, 4, 5]},
            "auxC":      {"source":             "scratch",
                          "name":               "_auxC"}
        }
    },
    "hy::updateSolutionHll": {
        "interface_file": "Hydro.h",
        "argument_list": ["lo", "hi", "U", "flX", "flY", "flZ"],
        "argument_specifications": {
            "lo":   {"source":           "tile_lo"},
            "hi":   {"source":           "tile_hi"},
            "U":    {"source":           "grid_data",
                     "structure_index":  ["CENTER", 1],
                     "RW":               [1, 2, 3, 4, 6],
                     "W":                [8]},
            "flX":  {"source":           "grid_data",
                     "structure_index":  ["FLUXX", 1],
                     "R":                [1, 2, 3, 4, 5]},
            "flY":  {"source":           "grid_data",
                     "structure_index":  ["FLUXY", 1],
                     "R":                [1, 2, 3, 4, 5]},
            "flZ":  {"source":           "grid_data",
                     "structure_index":  ["FLUXZ", 1],
                     "R":                [1, 2, 3, 4, 5]}
        }
    },
    "Eos::idealGammaDensIe": {
        "interface_file": "Eos.h",
        "argument_list": ["lo", "hi", "solnData"],
        "argument_specifications": {
            "lo":          {"source":           "tile_lo"},
            "hi":          {"source":           "tile_hi"},
            "solnData":    {"source":           "grid_data",
                            "structure_index":  ["CENTER", 1],
                            "R":                [1, 8],
                            "W":                [5, 7]}
        }
    }
}
